[QoISettings]
qoi = 14 
#The symbolic QoI ID. 
#14: pointwise or regional (specify function in qoi_string) 15: piecewise (specify function in qoi_string and qoi_string_2) 17: Q(|x|)
#Other ids are deprecated
qoi_string = x^2
#The expression of QoI. Should be univariate (x as the variable). Follow the style of https://github.com/symengine/symengine
qoi_string_2 = 0
#The second QoI expression used in qoi = 15 (piecewise) when x < threshold;
threshold = 0
#A threshold for the piece-wise QoI (qoi_string when x>=threshold, and qoi_string_2 when x<threshold)
#Also can be used as a isoline in other qois if make isolated = true;
isolated = false
#Set to true will create an isoline at threshold
#Following are the regional QoI parameters:
#qoiRegionMode = 0
#Whether to use regional QoI. 0: no region 1: regional average 2: laplacian 3: gradient length
#qoiRegionSize = 4
#The side length of QoI region. e.g., 4 is 4x4x4 block region for 3D data.
#confidence = 0.999
#The confidence (beta) parameter in paper. For SZ3, the recommended value is 0.99999.
error_std_rate = 2.0
#The c parameter in paper.
